#include <stdio.h>

int main() {
    int num,check=0;
    printf("Enter your number :");
    scanf("%d",&num);
    
    for(int i=2; i*i<=num; i++){
        if(num%i == 0){
            check = 1;
        }
    }
    if(check == 0){
        printf("%d is prime",num);
    } else {
        printf("%d is Not prime",num);
    }
    
    return 0;
}
